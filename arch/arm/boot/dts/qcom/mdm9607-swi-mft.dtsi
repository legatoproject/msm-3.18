		alias-80 = <&pm8019_gpios 3 (-1)>;
		alias-81 = <&pm8019_gpios 4 (-1)>;
		alias-82 = <&pm8019_gpios 6 (-1)>;
		alias-83 = <&pm8019_gpios 5 (-1)>;
