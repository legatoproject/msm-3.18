#include <dt-bindings/interrupt-controller/irq.h>

/ {
	model = "Sierra Wireless, Inc. MDM 9xxx WP series - mangOH Yellow";
	compatible = "qcom,mdm9607-mtp", "qcom,mdm9607", "qcom,mtp";
	qcom,board-id = <8 0x84>;
};

&soc {
	sierra_gpio {
		alias-TP500		= <300 0>;
		alias-WL_REG_ON		= <301 0>;
		alias-TP501		= <302 0>;
		alias-BT_DEV_WAKE	= <303 0>;
		alias-GENERIC_LED	= <304 0>;
		alias-PCM_EXP_SEL	= <305 0>;
		alias-TP502		= <306 0>;
		alias-TRI_LED_BLUE	= <307 0>;
		alias-CARD_DETECT_IOT0	= <308 0>;
		alias-BATT_GAUGE_GPIO	= <309 0>;
		alias-TRI_LED_RED	= <310 0>;
		alias-BAT_VOL_MONITOR	= <311 0>;
		alias-CHARGER_INT_N	= <312 0>;
		alias-MAG_DATA_READY	= <313 0>;
		alias-RTC_INT		= <314 0>;
		alias-TRI_LED_GREEN	= <315 0>;
	};
};


&i2c_4 {
	i2c-switch@71 {
		compatible = "nxp,pca9546";
		reg = <0x71>;
		#address-cells = <1>;
		#size-cells = <0>;

		i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;
		};
		i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;
		};
		i2c@2 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <2>;
		};
		i2c@3 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <3>;

			sx1509: gpio-expander@3E {
				compatible = "sx1509q";
				reg = <0x3E>;

				gpio-controller;
				#gpio-cells = <2>;

				sx150x,pullup_ena = <0x0>;
				sx150x,pulldn_ena = <0x0>;
				sx150x,float_ena = <0x0>;
				sx150x,polarity = <0x0>;
				sx150x,gpio_base = <300>;
			};
		};
	};
};
